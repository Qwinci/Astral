name=xkbfile
revision=1
from_source=xkbfile
imagedeps="meson ninja-build gcc"
hostdeps="xgcc xbinutils pkgconfig"
deps="base xorgproto libx11"

configure() {
	${source_dir}/configure --prefix=${prefix} --host=x86_64-astral 
}

build() {
	make -j${parallelism}
}

package() {
	DESTDIR=${dest_dir} make install
}
