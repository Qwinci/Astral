name=xcb-proto
revision=1
from_source=xcb-proto
imagedeps="build-essential python3"
hostdeps="xgcc xbinutils"
deps="base"

configure() {
	PYTHON=python3 ${source_dir}/configure --host=x86_64-astral --prefix=${prefix}
}

build() {
	make -j ${parallelism}
}

package() {
	make install DESTDIR=${dest_dir}
}
