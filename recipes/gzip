name=gzip
revision=1
from_source=gzip
imagedeps="build-essential"
hostdeps="xgcc xbinutils automake autoconf"
deps="base"

configure() {
	cp -rf ${source_dir}/* .
	./configure --host=x86_64-astral --prefix=${prefix}
}

build() {
	make -j ${parallelism}
}

package() {
	make install DESTDIR=${dest_dir}
}
