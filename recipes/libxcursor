name=libxcursor
revision=1
from_source=libxcursor
imagedeps="gcc"
hostdeps="xgcc xbinutils pkgconfig"
deps="base libxrender libxfixes libx11"

configure() {
	${source_dir}/configure --prefix=${prefix} --host=x86_64-astral
}

build() {
	make -j${parallelism}
}

package() {
	DESTDIR=${dest_dir} make install
}
