name=libfontenc
revision=1
from_source=libfontenc
imagedeps="gcc"
hostdeps="xgcc xbinutils pkgconfig util-macros"
deps="base zlib xorgproto"

configure() {
	${source_dir}/configure --prefix=${prefix} --host=x86_64-astral
}

build() {
	make -j${parallelism}
}

package() {

	make install DESTDIR=${dest_dir}

}
